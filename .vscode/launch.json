{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Polymer",
            "port": 9229,
            "protocol": "inspector",
            "runtimeExecutable": "polymer",
            "runtimeArgs": [
                "serve"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Carrousel",
            "url": "http://localhost:8081/demo",
            "runtimeArgs": [
              "--incognito"
          ],
            "webRoot": "${workspaceRoot}"
        }
    ],
    "compounds": [
        {
            "name": "Full-stack",
            "configurations": [
                "Polymer",
                "Carrousel"
            ]
        }
    ]
}